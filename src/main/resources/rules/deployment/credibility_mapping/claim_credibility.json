[
  {
    "name": "credible",
    "description": "checks the post and flags as credible",
    "priority": 2,
    "condition": "claimcredibility_tweet_claim_credibility_0_credibility  >= 0.5 && claimcredibility_tweet_claim_credibility_0_confidence > 0.6",
    "actions": [
      "callback.getModuleCredibility().put(\"claim_similarity\", Credibility.credible);"
    ]
  },
  {
    "name": "mostly_credible",
    "description": "checks the post and flags as mostly credible",
    "priority": 3,
    "condition": "claimcredibility_tweet_claim_credibility_0_credibility < 0.5 && claimcredibility_tweet_claim_credibility_0_credibility >= 0.25 && claimcredibility_tweet_claim_credibility_0_confidence > 0.6",
    "actions": [
      "callback.getModuleCredibility().put(\"claim_similarity\", Credibility.mostly_credible);"
    ]
  },
  {
    "name": "mostly_not_credible",
    "description": "checks the post and flags as mostly not credible",
    "priority": 3,
    "condition": "claimcredibility_tweet_claim_credibility_0_credibility < 0.25  && claimcredibility_tweet_claim_credibility_0_credibility >= -0.5 && claimcredibility_tweet_claim_credibility_0_confidence >= 0.6",
    "actions": [
      "callback.getModuleCredibility().put(\"claim_similarity\", Credibility.mostly_not_credible);"
    ]
  },
  {
    "name": "not_credible",
    "description": "checks the post and flags as not credible",
    "priority": 2,
    "condition": "claimcredibility_tweet_claim_credibility_0_credibility < -0.5 && claimcredibility_tweet_claim_credibility_0_confidence >= 0.6",
    "actions": [
      "callback.getModuleCredibility().put(\"claim_similarity\", Credibility.not_credible);"
    ]
  },
  {
    "name": "not_verifiable",
    "description": "checks the post and flags as not verifiable",
    "priority": 1,
    "condition": "true || claimcredibility_tweet_claim_credibility_0_confidence < 0.6",
    "actions": [
      "callback.getModuleCredibility().put(\"claim_similarity\", Credibility.not_verifiable);"
    ]
  },
  {
    "name": "set_values",
    "description": "set the Module cred/conf values",
    "priority": 7,
    "condition": "true",
    "actions": [
      "callback.setConfidence(\"claim_similarity\", claimcredibility_tweet_claim_credibility_0_confidence);",
      "callback.setCredibility(\"claim_similarity\", claimcredibility_tweet_claim_credibility_0_credibility);"
    ]
  }
]
