    "name": "credible",
    "description": "checks the post and flags as credible",
    "priority": 2,
    "condition": "credibility  > 0.5 && confidence > 0.7",
    "actions": [
      "callback.getModuleCredibility().put(\"claim_similarity\", Credibility.not_credible_post);"
    ]
  },
  {
    "name": "mostly credible_post",
