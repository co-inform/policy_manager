[
  {
    "name": "not_verifiable_post_content_analysis",
    "description": "checks the post and flags as not verifiable",
    "priority": 1,
    "condition": "contentanalysis_veracity_true < 0.15 && contentanalysis_veracity_unknown >= 0.50",
    "actions": [
      "callback.getModuleCredibility.put(\"content_analysis\", Credibility.not_verifiable);"
    ]
  },
  {
    "name": "not-credible_post_content_analysis",
    "description": "checks the post and flags as not credible",
    "priority": 2,
    "condition": "contentanalysis_veracity_true < 0.10 && contentanalysis_veracity_unknown > 0.4 && contentanalysis_veracity_unknown < 0.33",
    "actions": [
      "callback.getModuleCredibility().put(\"content_analysis\", Credibility.not_credible);"
    ]
  },
  {
    "name": "credible_uncertain_post_content_analysis",
    "description": "checks the post and flags as credible uncertain",
    "priority": 3,
    "condition": "contentanalysis_veracity_true < 0.20 && contentanalysis_veracity_true >= 0.15 && contentanalysis_veracity_unknown >= 0.33",
    "actions": [
      "callback.getModuleCredibility().put(\"content_analysis\", Credibility.credible_uncertain);"
    ]
  },
  {
    "name": "mostly_credible_post_content_analysis",
    "description": "checks the post and flags as mostly credible",
    "priority": 4,
    "condition": "contentanalysis_veracity_true < 0.33 && contentanalysis_veracity_true >= 0.20 && contentanalysis_veracity_unknown <= 0.33",
    "actions": [
      "callback.getModuleCredibility().put(\"content_analysis\", Credibility.mostly_credible);"
    ]
  },
  {
    "name": "credible_post_content_analysis",
    "description": "checks the post and flags as credible",
    "priority": 5,
    "condition": "contentanalysis_veracity_true >= 0.33 && contentanalysis_veracity_unknown <= 0.33",
    "actions": [
      "callback.getModuleCredibility().put(\"content_analysis\", Credibility.credible);"
    ]
  },
    {
    "name": "mostly_not_credible_post_content_analysis",
    "description": "checks the post and flags as credible",
    "priority": 6,
    "condition": "contentanalysis_veracity_true < 0.15 && contentanalysis_veracity_unknown <= 0.33",
    "actions": [
      "callback.getModuleCredibility().put(\"content_analysis\", Credibility.credible);"
    ]
  }
